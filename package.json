{
	"name": "go-auto-struct-tag",
	"displayName": "Go Auto Struct Tag",
	"description": "Auto complete struct tag for Go",
	"keywords": [
		"go",
		"golang",
		"auto",
		"struct",
		"tag",
		"complete",
		"json"
	],
	"version": "0.0.1",
	"publisher": "vivaldy22",
	"repository": {
		"type": "git",
		"url": "https://github.com/vivaldy22/go-auto-struct-tag"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Programming Languages",
		"Extension Packs"
	],
	"activationEvents": [
		"onLanguage:go"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}